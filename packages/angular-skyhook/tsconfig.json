{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "module": "es2015",
        "moduleResolution": "node",
        "inlineSources": true,
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "skipLibCheck": true,
        "stripInternal": true,
        "lib": ["es2015", "dom"]
    },
    "angularCompilerOptions": {
        "annotateForClosureCompiler": false
    }
}
