{{#if isVisible}}
    {{#if isLabel}}
        <li class="label {{cssClasses}}">
            <span>{{{wbr title}}}</span>
        </li>
    {{else}}
        {{#if isGlobals}}
            {{!-- <li class="globals {{#if isInPath}}current{{/if}} {{cssClasses}}">
                <a href="{{relativeURL url}}"><em>{{{wbr title}}}</em></a>
            </li> --}}
            <li class="globals {{cssClasses}}">
                <a href="/index.html"><em>{{{wbr 'Home'}}}</em></a>
            </li>
        {{else}}
            <li class="{{#if isInPath}}current{{/if}} {{cssClasses}}">
                <a href="{{relativeURL url}}">{{{removeHyphens title}}}</a>
                {{#if isInPath}}
                    {{#if toc}}
                        <nav class="tsd-navigation secondary">
                            <ul class="before-current">
                                {{#each toc.children}}
                                    {{> toc.root}}
                                {{/each}}
                            </ul>
                        </nav>
                    {{/if}}
                {{/if}}
                {{#if isInPath}}
                    {{#if children}}
                        <ul>
                            {{#each children}}
                                {{> navigation}}
                            {{/each}}
                        </ul>
                    {{/if}}
                {{/if}}
            </li>
        {{/if}}
    {{/if}}
{{/if}}
